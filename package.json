    "gatsby-plugin-google-analytics": "^3.6.0",
    "gatsby-plugin-manifest": "^3.6.0",
    "gatsby-plugin-netlify": "^3.6.1",
    "gatsby-plugin-offline": "^4.6.0",
    "gatsby-plugin-preact": "^5.10.0",
    "gatsby-plugin-sitemap": "^4.2.0",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.22",
    "preact": "^10.5.14",
    "preact-render-to-string": "^5.1.19",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@netlify/plugin-gatsby": "^1.0.3",
    "@netlify/plugin-lighthouse": "^2.1.2",
    "cross-env": "^7.0.3"
  }
